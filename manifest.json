{
    "manifest_version": 3,
    "name": "Eye typing extention",
    "description": "Base Level Extension",
    "version": "1.0",
    "action": {
        "default_popup": "index.html",
        "default_icon": "mic.svg"
    },
    "permissions": [
        "activeTab",
        "audio",
        "scripting",
        "tabs"
    ],
    "background": {
        "service_worker": "background.js"
    }
}